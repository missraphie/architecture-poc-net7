{
  "GlobalConfiguration": {
    "BaseUrl": "https://gateway.xacte.local",
    "RequestIdKey": "Xacte-Oc-Request-Id"
  },
  "Routes": [
    {
      "Key": "PATIENTS",
      "UpstreamPathTemplate": "/gateway/patients",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/patients",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "patient.xacte.local",
          "Port": 443
        }
      ]
    },
    {
      "Key": "PATIENT",
      "UpstreamPathTemplate": "/gateway/patients/{id}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/patients/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "patient.xacte.local",
          "Port": 443
        }
      ]
    },
    {
      "Key": "PATIENT_BILLING",
      "UpstreamPathTemplate": "/gateway/patients/{id}/billings",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/patients/{id}/billings",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "patient.xacte.local",
          "Port": 443
        }
      ]
    }
  ],
  "Aggregates": [
    {
      "RouteKeys": [
        "PATIENT",
        "PATIENT_BILLING"
      ],
      "UpstreamPathTemplate": "/aggregate/patient",
      "Aggregator": "PatientAggregator"
    }
  ]
}